<script>

  $('#sidebar-toggle').click(function() {
    var text = $(this).text();
    console.log(text)
    if ($(this).text() === 'arrow_back') {
      $(this).text('arrow_forward')
    } else  if ($(this).text() === 'arrow_forward') {
      $(this).text('arrow_back')
    }
  })
  
  $.getJSON('/allRealtors', function(teacherData) {
      console.log(teacherData);
      teacherData.forEach(function(teacher) {
        $('#example1 tbody').append(`
                <tr id='${teacher._id}' class="teacherRow">
                    <td class="name">${teacher.name}</td>
                    <td class="email">${teacher.local.email}</td>
                    <td class="district">${teacher.phone}</td>
                    `);
    })
  })

  $('.addTeacher').click(function() {
      var name = $('input#name').val();
      var email = $('input#email').val();
      var phone = $('input#phone').val();
      var password = $('select#pw').val();
        $.ajax({
            url: "/addRealtor",
            method: "POST",
            data: {
                group: 3,
                local: {
                    email,
                    password
                },
                name,
                phone
            },
            success: function(res) {
                console.log('respnse', res);
                location.reload();
            }
        });
  })

</script>
